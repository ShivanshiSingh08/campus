const express = require('express');
const app = express();
const PORT = process.env.PORT || 3000;

// Mock data for Christ University campuses
const campuses = [
  {
    name: "Christ University Central Campus",
    location: "Bangalore",
    facilities: ["Library", "Hostel", "Sports Complex", "Auditorium"],
    imgUrl: "data:image/jpeg;base64,",
    id: "1"
  },
  {
    name: "Christ University Lavasa Campus",
    location: "Pune",
    facilities: ["Library", "Hostel", "Gym", "Cafeteria"],
    imgUrl: "data:image/jpeg;base64,",
    id: "2"
  },
  {
    name: "Christ University Noida Campus",
    location: "Noida",
    facilities: ["Library", "Hostel", "Basketball Court", "Amphitheater"],
    imgUrl: "data:image/jpeg;base64,",
    id: "3"
  },
  {
    name: "Christ University Yeshwanthpur Campus",
    location: "Yeshwanthpur",
    facilities: ["Library", "Hostel", "Cricket Ground", "Laboratories"],
    imgUrl: "data:image/jpeg;base64,",
    id: "4"
  },
  {
    name: "Christ University Bannerghatta Campus",
    location: "Bannerghatta",
    facilities: ["Library", "Hostel", "Football Ground", "Medical Center"],
    imgUrl: "data:image/jpeg;base64,",
    id: "5"
  }
];

// Endpoint to get all campuses
app.get('/api/campuses', (req, res) => {
  res.json(campuses);
});

// Endpoint to get campus by ID
app.get('/api/campuses/:id', (req, res) => {
  const campusId = req.params.id;
  const campus = campuses.find(campus => campus.id === campusId);
  if (campus) {
    res.json(campus);
  } else {
    res.status(404).json({ message: "Campus not found" });
  }
});

app.listen(PORT, () => {
  console.log(`Server is running on http://localhost:${PORT}`);
});
